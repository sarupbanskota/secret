.row{style: "background-color: #2e2e2e"}
	.col-md-1
	.col-md-10
		%h3{style: "color: whitesmoke"} Speaker Dashboard
		%br
		.row
			.col-md-4
				.well{style: "background-color: #2e2e2e"}
					%h3 Profile Information
					= render 'speaker_overview'
				/
					.well{style: "background-color: #2e2e2e"}
						%h3 Announcements
						= render 'speaker_announcements'
			.col-md-8.well{style: "background-color: #2e2e2e"}
				- if @seminars.count > 0
					%h3 Proposals
					= render 'seminars/index'
				- else
					.btn.btn-lg.btn-success= link_to "Submit new Proposal", "/seminars/new", style: "color: white"
